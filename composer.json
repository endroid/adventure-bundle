{
    "name": "endroid/adventure-bundle",
    "description": "Endroid Adventure Bundle",
    "keywords": ["endroid", "adventure", "symfony", "bundle", "php"],
    "homepage": "https://github.com/endroid/adventure-bundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "doctrine/doctrine-bundle": "^1.8.1||^2.0",
        "doctrine/orm": "^2.6",
        "endroid/installer": "^1.2.2",
        "ramsey/uuid": "^4.0",
        "symfony/asset": "^4.4||^5.0",
        "symfony/messenger": "^4.4||^5.0",
        "symfony/yaml": "^4.4||^5.0",
        "twig/twig": "^2.4"
    },
    "require-dev": {
        "endroid/quality": "dev-master"
    },
    "suggest": {
        "roave/security-advisories": "Avoids installation of package versions with vulnerabilities",
        "symfony/security-checker": "Checks your composer.lock for vulnerabilities"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Endroid\\AdventureBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endroid\\AdventureBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
